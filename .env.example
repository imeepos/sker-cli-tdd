# OpenAI API 配置
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4
OPENAI_BASE_URL=https://api.openai.com/v1
OPENAI_MAX_TOKENS=1000
OPENAI_TEMPERATURE=0.7

# MCP 服务器配置
MCP_SERVER_NAME=sker-ai-mcp-server
MCP_SERVER_VERSION=1.0.0

# 工作空间配置
DEFAULT_WORKSPACE=global
AUTO_SAVE_CONFIG=true

# ===========================================
# MQ Agent 系统配置
# ===========================================

# MQ连接URL (推荐使用，格式: amqp://username:password@host:port)
MQ_URL=amqp://guest:guest@localhost:5672

# 或者使用分离的配置 (如果没有设置MQ_URL)
# MQ_HOST=localhost
# MQ_PORT=5672
# MQ_USERNAME=guest
# MQ_PASSWORD=guest

# 任务队列名称
MQ_TASK_QUEUE=task_queue

# 结果队列名称
MQ_RESULT_QUEUE=result_queue

# Agent唯一标识符
AGENT_ID=agent-001

# 任务处理超时时间 (毫秒)
TASK_TIMEOUT=30000

# 最大并发任务数
MAX_CONCURRENT_TASKS=10

# 允许的任务类型 (逗号分隔)
ALLOWED_TASK_TYPES=execute_command,read_file,write_file,get_system_info,fetch_url

# ===========================================
# MQ_URL 格式示例
# ===========================================

# 本地开发环境
# MQ_URL=amqp://guest:guest@localhost:5672

# 生产环境 (RabbitMQ)
# MQ_URL=amqp://prod_user:prod_pass@rabbitmq.production.com:5672

# 云服务 (CloudAMQP)
# MQ_URL=amqps://username:password@instance.cloudamqp.com/vhost

# 集群环境
# MQ_URL=amqp://cluster_user:cluster_pass@rabbitmq-cluster.internal:5672
